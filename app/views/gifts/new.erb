<div class="fixed-action-btn">
    <a class="btn-floating btn-large red tooltipped" onclick="history.go(-1)"
    data-position="left" data-delay="50" data-tooltip="Back">
      <i class="large material-icons">arrow_back</i>
    </a>
</div>

<main class="loginform container">   
  <div class="row">
    <div class="col s6"><h4>Create New Gift</h4></div>
  </div>   
  <form class="col s12" action="/users/<%=@user.id%>/gifts" method="POST">
    <%= erb :'gifts/_form', locals: {gift: @gift} %>
  </form>
</main>


